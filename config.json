{
  "folders": {
    "images": "images",
    "pdf_catalog": "catalog",
    "metadata": "metadata-files"
  },
  "image_formats": [".jpg", ".jpeg", ".png", ".bmp", ".gif", ".webp"],
  "model": {
    "_comment": "Available pretrained: openai, laion2b_s32b_b82k, datacomp_xl_s13b_b90k, laion400m_e32",
    "name": "clip-vit-b-32",
    "architecture": "ViT-B-32",
    "pretrained": "openai", 
    "embedding_dim": 512
  },
  "search": {
    "top_k": 8,
    "batch_size": 8
  },
  "pdf": {
    "max_keywords": 5,
    "jpeg_quality": 99,
    "ai_analysis": {
      "enabled": true,
      "model": "llama3.2:3b",
      "ollama_url": "http://172.18.128.1:11434",
      "fields": ["Project Name", "Location", "Client", "Contractor", "Date of Completion", "Role", "Description"],
      "caption_template": "{project_name}"
    }
  }
}
